<tr class='nested-fields'>
  <td>
    <% if defined?(vista) %>
      <%= render partial: 'campo_nombrecampo', 
        locals: {f: f, vista: vista} %>
    <% else %>
      <%= render partial: 'campo_nombrecampo', 
        locals: {f: f} %>
    <% end %>
  </td>
  <td>
    <%= f.input :columna,
      collection: ('A'..'CZ').to_a,
      label: false
    %>
  </td>
  <td>
    <%= f.input :id, as: :hidden %>
    <%= link_to_remove_association "Eliminar", f, 
        :class => 'btn-danger' 
    %>
    <div><%= f.error_notification %></div>
  </td>
</tr>
